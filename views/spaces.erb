<h1>Welcome <%= @current_user.username %></h1>


<form action='/spaces/new'>
  <input type='submit' value='add'>
</form>

<ul>
  <li> My beautiful Treehouse </li>
  <% @spaces.each do |space| %>
  <li><%= "#{space.name}, #{space.description}, #{space.price}, available from: #{space.available_from}, to: #{space.available_to}"%></li>
  <% end %>
</ul>
